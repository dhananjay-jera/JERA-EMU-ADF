{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_Fas_BE_Stg_DB_ModelDB_test",
						"type": "DatasetReference"
					},
					"name": "source1"
				},
				{
					"dataset": {
						"referenceName": "dt_Fas_Be_Dev_DB_ModelDB_test_fas_be_stg",
						"type": "DatasetReference"
					},
					"name": "source2"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "AddMD5"
				},
				{
					"name": "derivedColumn1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          Date as date,",
				"          Time as timestamp,",
				"          Data1 as string,",
				"          Data2 as double,",
				"          Data3 as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     limit: 100,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> source1",
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     limit: 100,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> source2",
				"source1 derive(md5_hash = md5(toString(Date) + toString(Time) + Data1 + toString(Data2) + toString(Data3))) ~> AddMD5",
				"source2 derive(md5_hash = ERROR_FUNCTION('')) ~> derivedColumn1"
			]
		}
	}
}